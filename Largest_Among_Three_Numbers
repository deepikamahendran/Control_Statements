package demo;

import java.util.Scanner;

public class Largest_No_of_Three 
{

	public static void main(String[] args) 
	{
		Scanner s = new Scanner(System.in);
		int a = s.nextInt();
		int b= s.nextInt();
		int c = s.nextInt();
		if(a>b&&a>c)
		{
			System.out.println("A is largest among three values : "+a);
		}
		else if(b>a&&b>c)
		{
			System.out.println("B is largest among three values : "+b);
		}
		else
		{
			System.out.println("C is largest among three values : "+c);
		}

	}

}
